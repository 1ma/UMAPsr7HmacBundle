{
    "name": "uma/psr7-hmac-bundle",
    "description": "Clean integration between Psr7Hmac and the Symfony Security component",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["http", "psr7", "hmac"],
    "homepage": "https://github.com/1ma/UMAPsr7HmacBundle",
    "support": {
        "issues": "https://github.com/1ma/UMAPsr7HmacBundle/issues",
        "source": "https://github.com/1ma/UMAPsr7HmacBundle"
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/config": "^2.7|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/http-kernel": "^2.7|^3.0",
        "symfony/psr-http-message-bridge": "^0.2.0",
        "symfony/security-bundle": "^2.7|^3.0",
        "uma/psr7-hmac": "^0.5.1",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.2",
        "symfony/framework-bundle": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0"
    },
    "autoload": {
        "psr-4": {
            "UMA\\Psr7HmacBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestProject\\": "tests/TestProject/src/",
            "UMA\\Tests\\Psr7HmacBundle\\": "tests/Functional"
        },
        "classmap": [
            "tests/TestProject/app/TestKernel.php"
        ]
    }
}
